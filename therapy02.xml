<?xml version="1.0" encoding="UTF-8"?>
<evaml id="a1001-b3001" name="Therapy02">
	<settings>
		<voice tone="en-US_AllisonV3Voice" />
		<lighting-effects mode="on" />
		<audio-effects mode="on" vol="100%" />
	</settings>
	<script>
    <random id="BEGIN" min="1" max="3"/>
    <switch>
      <case value="1">
        <light color="ff0000" state="on"/>
      </case>
      <case value="2">
        <light color="00ff00" state="on"/>
      </case>
      <case value="3">
        <light color="0000ff" state="on"/>
      </case>
    </switch>
    <wait duration="500"/>
    <goto target="BEGIN"/>
	</script>
  <macros>
  </macros>
</evaml>
