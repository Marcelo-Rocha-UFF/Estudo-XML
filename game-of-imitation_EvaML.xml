<evaml name="game-of-imitation" id="0196aac68905bb2013f247c854962414">
	<settings>
		<voice tone="pt-BR_IsabelaV3Voice" key="1000" />
		<light-effects mode="on" />
		<audio-effects mode="on" vol="100%" />
	</settings>
	<script>
		
		<light state="on" color="white" key="1001" />
