		<evaEmotion emotion="happy" key="1018" />
		<light state="on" color="#ffc000" key="1019" />
		<audio source="mario-sound1" block="true" key="1020" />
		<talk key="1021">Você pode me imitar?</talk>
		<audio source="mario-fundo" block="false" key="1022" />
		<userEmotion key="1023" />
		<switch var="$">
			<case op="exact" value="happy" key="1024" var="$">
				<audio source="mario-start2" block="true" key="1025" />
				<talk key="1026">Parabéns, você acertou!</talk>
			</case>
			<default key="1027" var="$" value="" op="exact">
				<audio source="mario-game-intro" block="true" key="1028" />
			</default>
		</switch>

		
		<evaEmotion emotion="neutral" key="1029" />
		<light state="on" color="white" key="1030" />
		<talk key="1031">Vou demonstrar outra emoção</talk>
		<audio source="mario-game-intro" block="true" key="1032" />
		<talk key="1033">Eu me sinto com raiva</talk>
		<evaEmotion emotion="angry" key="1034" />
		<light state="on" color="#ff0000" key="1035" />
		<audio source="mario-sound1" block="true" key="1036" />
		<talk key="1037">Você pode me imitar?</talk>
		<audio source="mario-fundo" block="false" key="1038" />
		<userEmotion key="1039" />
		<switch var="$">
			<case op="exact" value="angry" key="1040" var="$">
				<audio source="mario-start2" block="true" key="1041" />
				<talk key="1042">Parabéns, você acertou!</talk>
			</case>
			<default key="1043" var="$" value="" op="exact">
				<audio source="mario-game-intro" block="true" key="1044" />
			</default>
		</switch>	

		
		<evaEmotion emotion="neutral" key="1045" />
		<light state="on" color="white" key="1046" />
		<talk key="1047">Vou demonstrar outra emoção</talk>
		<audio source="mario-game-intro" block="true" key="1048" />
		<talk key="1049">Eu me sinto triste</talk>
		<evaEmotion emotion="sad" key="1050" />
		<light state="on" color="blue" key="1051" />
		<audio source="mario-sound1" block="true" key="1052" />
		<talk key="1053">Você pode me imitar?</talk>
		<audio source="mario-fundo" block="false" key="1054" />
		<userEmotion key="1055" />
		<switch var="$">
			<case op="exact" value="sad" key="1056" var="$">
				<audio source="mario-start2" block="true" key="1057" />
				<talk key="1058">Parabéns, você acertou!</talk>
			</case>
			<default key="1059" var="$" value="" op="exact">
				<audio source="mario-game-intro" block="true" key="1060" />
			</default>
		</switch>		
		<evaEmotion emotion="happy" key="1061" />
		<light state="on" color="white" key="1062" />
		<talk key="1063">Foi muito bom jogar com você $1</talk>
		<audio source="mario-game-fim" block="true" key="1064" />
		<light state="off" key="1065" />
