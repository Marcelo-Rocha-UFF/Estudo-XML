<evaml name="script13">
	<settings>
		<voice tone="en-US_AllisonV3Voice" />
		<light-effects mode="on" />
		<audio-effects mode="on" vol="100%" />
	</settings>
	<script>
		<random min="1" max="1" />
		<counter var="x" op="=" value="1" />
		<switch var="x">
			<case op="eq" value="$">
				<light state="on" color="green" />
			</case>
			<default>
				<light state="on" color="red" />
			</default>
		</switch>
	</script>
	<macros>
	</macros>
</evaml>
