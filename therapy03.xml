<?xml version="1.0" encoding="UTF-8"?>
<evaml id="a1001-b3001" name="Therapy03">
	<settings>
		<voice tone="en-US_AllisonV3Voice" />
		<light-effects mode="on" />
		<audio-effects mode="on" vol="100%" />
	</settings>
  <script>
    <random id="INICIO" min="1" max="3"/>
    <switch>
      <case value="1">
      	<wait duration="1000"/>
      	<switch>
        	<case value="2">
          	<wait duration="2000"/>
          	<goto target="LISTEN"/>
        	</case>
        	<case value="4">
          	<wait duration="4000"/>
        	</case>
      	</switch>
      </case>
      <case value="3">
      	<listen id="LISTEN"/>
      </case>
    </switch>
    <goto target="INICIO"/>
	</script>
  <macros>
  </macros>
</evaml>
