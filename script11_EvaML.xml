<evaml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="script11" xsi:noNamespaceSchemaLocation="/home/marcelo/Dropbox/Estudo XML/EvaML-Schema/evaml_schema.xsd" id="366eded09c80eac224e5b14407369513">
  <settings>
      <voice tone="en-US_AllisonV3Voice" key="1000" />
      <lightEffects mode="on" />
      <audioEffects mode="on" vol="100%" />
