<evaml name="script11" id="366eded09c80eac224e5b14407369513">
	<settings>
		<voice tone="en-US_AllisonV3Voice" key="1000" />
		<lightEffects mode="on" />
		<audioEffects mode="on" volume="100" />
	</settings>
	<script>
		<talk key="1001">Hi! I am the robot EVA</talk>
		<light state="on" color="blue" key="1002" />
		<counter var="x" op="=" value="1" key="1003" />
		<switch var="x">
			<case op="lte" value="0" key="1004" var="x">
				<talk key="1005">Ok</talk>
			</case>
			<default key="1006" var="x" value="" op="exact">
				<talk key="1007">Wrong</talk>
			</default>
		</switch>
	</script>
	<links><link from="1000" to="1001" /><link from="1001" to="1002" /><link from="1002" to="1003" /><link from="1003" to="1004" /><link from="1004" to="1005" /><link from="1006" to="1007" /></links></evaml>