<evaml name="script02" id="8720e5178266b4dc1173d6ad62863b4d">
	<settings>
		<voice tone="en-US_AllisonV3Voice" key="1000" />
		<light-effects mode="on" />
		<audio-effects mode="on" vol="100%" />
	</settings>
<script>
  <random min="1" max="5" key="1001" />
  <switch var="$">
    <case op="eq" value="1" key="1002" var="$">
      <talk key="1003">The number drawn was 1</talk>
    </case>
    <case op="eq" value="2" key="1004" var="$">
      <talk key="1005">The number drawn was 2</talk>
    </case>
    <default key="1006" var="$" value="" op="exact">
      <talk key="1007">The number drawn was neither 1 nor 2</talk>
      <light state="on" color="green" key="1008" />
    </default>
  </switch>
</script>
<links><link from="1000" to="1001" /><link from="1001" to="1002" /><link from="1002" to="1003" /><link from="1001" to="1004" /><link from="1004" to="1005" /><link from="1001" to="1006" /><link from="1006" to="1007" /><link from="1007" to="1008" /></links></evaml>